
page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.type-container {
  width: 90%;
  padding: 10rpx;
  font-size:14px;
}
.type-input input, .sample-container input{
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  border: 1rpx solid #eee;
  padding-left: 10rpx;
  border-radius: 8rpx;
  min-height: 60rpx;
  width: 40%;
}
.process-button{
  border:1rpx solid #888;
  border-radius:100%;
  width:50rpx;
  height:50rpx;
  text-align:center;
  box-shadow:1rpx;
  line-height:50rpx;
  float:right;
}
.sample-container text{
  margin:0 10rpx;
}